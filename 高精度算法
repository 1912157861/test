#include<iostream>
#include<string>
#include<vector>
//高精度加法是一个大数A乘以小数b
using namespace std;

vector<int> mul(vector<int> &A, int b){
	vector<int> C;
	int t = 0;
	for (int i = 0; i < A.size() || t; i++){
		if(i<A.size())  t = t + A[i] * b;
		C.push_back(t % 10);
		t = t / 10;
	}
	return C;
}

int main(){
	string a;
	int b;
	vector<int> A;
	cin >> a >> b;
	for (int i = A.size() - 1; i >= 0; i--){
		A.push_back(a[i]);
	}

	auto C = mul(A, b);

	for (int i = C.size(); i >= 0; i--){
		printf("%d", C[i]);
	}
	return 0;
}
